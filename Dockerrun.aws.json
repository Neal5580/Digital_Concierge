{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "cms",
            "image": "nealshen/dc-cms",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "nealshen/dc-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "nealshen/dc-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["cms", "server"],
            "memory": 128
        }
    ]
}